{
    "name": "crisu83/yiistrap-widgets",
    "description": "Extension library for Yiistrap.",
    "keywords": ["yiistrap", "widgets", "bootstrap", "yii"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christoffer Niska",
            "email": "christoffer.niska@gmail.com",
            "homepage": "http://www.cniska.net/"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ivaynberg/select2",
                "version": "3.4.3",
                "dist": {
                    "url": "https://github.com/ivaynberg/select2/archive/3.4.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jhollingworth/bootstrap-wysihtml5",
                "version": "dev-master",
